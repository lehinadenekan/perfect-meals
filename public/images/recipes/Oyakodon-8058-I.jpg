    margin: 0 auto var(--wp--custom--layout--block-gap);
}
</style>
<style id='iwpblocks-loop-latest-style-inline-css'>
.loop-latest {
    margin: 0;
}

.loop-latest .wrap {
    padding: 0
}
</style>
<style id='iwpblocks-loop-numbered-style-inline-css'>
.loop-numbered {
    margin: 0;
}

.loop-numbered .wrap {
    position: relative;
}

.section-numbered {
    display: grid;
    column-gap: var(--wp--custom--layout--block-gap);
    row-gap: var(--wp--custom--layout--block-gap);
    grid-template-columns: repeat(5, minmax(0, 1fr));
}
/*--------------------------------------------------------------
Post Format: Numbered
--------------------------------------------------------------*/
.post-numbered .abbr-content {
    padding: 0 var(--wp--custom--layout--block-gap-small) var(--wp--custom--layout--block-gap-small);
    position: relative;
}

.post-numbered h3 {
    margin: 0 0 var(--wp--custom--layout--block-gap-small) !important;
    font-weight: 700;
    font-size: var(--wp--preset--font-size--medium);
    line-height: var(--wp--custom--line-height--xsmall);
    display: block;
}

.count-number {
    width: 25px;
    height: 25px;
    font-size: var(--wp--preset--font-size--xsmall);
    font-weight: 700;
    line-height: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    background: var(--wp--preset--color--red);
    color: var(--wp--preset--color--white);
    margin: -15px 0 5px;
}
/*--------------------------------------------------------------
Sidebar Styling
--------------------------------------------------------------*/
.block-area-sidebar .loop-numbered {
    margin: 0 auto var(--wp--custom--layout--block-gap);
}

/*--------------------------------------------------------------
Responsive Styling
--------------------------------------------------------------*/
@media (max-width: 900px) {
    .section-numbered {
        grid-template-columns: repeat(2, minmax(0, 1fr));
    }
}

@media (max-width: 600px) {
    .loop-numbered {
        padding: var(--wp--custom--layout--block-gap);
    }
}
</style>
<style id='iwpblocks-loop-tabbed-style-inline-css'>
.loop-tabbed {
    margin: 0;
    padding: 0;
}
.loop-tabbed .section-header {
    justify-content: space-between;
}
.loop-tabbed .section-title-group {
    display: flex;
    align-items: center;
    justify-content: flex-start;
}

.tab-buttons {
    max-width: 50%;
    display: grid;
    column-gap: var(--wp--custom--layout--block-gap-small);
    row-gap: var(--wp--custom--layout--block-gap-small);
    position: relative;
    grid-template-columns: repeat(4, minmax(0, 1fr));
}
.tab-buttons:hover {
    opacity:1;
}
.tab-buttons a {
    font-weight: 700;
    font-size: var(--wp--preset--font-size--xxsmall);
    letter-spacing: var(--wp--custom--letter-spacing--loose);
    line-height: var(--wp--custom--line-height--small);
    text-transform: uppercase;
    padding: 10px;
    text-align: center;
    border: 2px solid var(--wp--preset--color--ink);
    color: var(--wp--preset--color--ink)!important;
}

.tab-button.active {
    background: var(--wp--preset--color--ink);
    color: var(--wp--preset--color--white)!important;
}
.tabbed-grid {
    display: grid;
    column-gap: var(--wp--custom--layout--block-gap-small);
    row-gap: var(--wp--custom--layout--block-gap-small);
    position: relative;
    grid-template-columns: repeat(4, minmax(0, 1fr));
}
@media (max-width: 1100px) {
    .loop-tabbed .section-header {
        flex-wrap: wrap;
    }
}
@media (max-width: 800px) {
    .loop-tabbed .section-header {
        display:block;
    }
    .loop-tabbed .section-title-group {
        justify-content: center;
    }
    .tabbed-grid {
        grid-template-columns: repeat(2, minmax(0, 1fr));
    }
    .tab-buttons {
        grid-template-columns: repeat(2, minmax(0, 1fr));
        max-width: 100%;
    }
}
@media (max-width: 760px) {
    .loop-tabbed .section-title-group {
        display: block;
        text-align:center;
        margin: 0 auto 10px;
    }
}
</style>
<style id='acf-post-curated-style-inline-css'>
